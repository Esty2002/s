                    "Leads CRUD operations"
                ],
                "description": "get record",
                "operationId": "getrecord",
                "parameters": [
                    {
                        "name": "table",
                        "in": "path",
                        "schema": {
                            "$ref": "#/components/schemas/tableName"
                        },
                        "required": true,
                        "description": "Table name"
                    },
                    {
                        "name": "columns",
                        "in": "path",
