                                "type": "string",
                                "description": "mapReference",
                                "example": "mercaz"
                            }
                        }
                    },
                    "clientId": {
                        "type": "string",
                        "description": "clientId",
                        "example": "112233"
                    },
                    "concretType": {
                        "type": "list",
                        "description": "concretType",
                        "example": "false"
                    },
                    "concretAmount": {
                        "type": "double",
                        "description": "concretAmount",
                        "example": "15.1"
                    },
                    "moreConcretItems": {
                        "type": "string",
                        "description": "moreConcretItems",
                        "example": "asd"
                    },
                    "concretAdditionIsProsprective": {
                        "type": "Boolean",
                        "description": "concretAdditionIsProsprective",
                        "example": "true"
                    },
                    "pumpId": {
                        "type": "string",
                        "description": "pumpId",
                        "example": "pipe"
                    },
                    "morePumpItems": {
                        "type": "string",
                        "description": "morePumpItems",
                        "example": "afef"
                    },
                    "pouringType": {
                        "type": "string",
                        "description": "pouringType",
                        "example": "asf"
                    },
                    "notes": {
                        "type": "string",
                        "description": "notes",
                        "example": "SAsadsdsfdsfdf"
                    }
                }
            },
          
            "updateleadsdetails": {
                "type": "object",
                "properties": {
                    "filter": {
                        "type": "object",
                        "properties": {
                            "serialNumber": {
                                "type": "int",
                                "example": "1"
                            }
                        }
                    },
                    "obj": {
                        "type": "object",
                        "properties": {
                            "leadStatus": {
                                "type": "string",
                                "example": "ישן"
                            }
                        }
                    }
