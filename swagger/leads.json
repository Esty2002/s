    "components": {
        "schemas": {
            "puringType": {
                "type": "object",
                "properties": {
                    "serialNumber": {
                        "type": "int",
                        "description": "The number of serial",
                        "example": "1234"
                    },
                    "name": {
                        "type": "string",
                        "description": "The name of puringType(50 chars)",
                        "example": "Sara coen"
                    },
                    "addedDate": {
                        "type": "date",
                        "description": "The date of the puringType's first",
                        "example": "03-20-2023"
                    },
                    "disable": {
                        "type": "boolean",
                        "description": "Is the orderer able to make an puringtype?",
                        "example": "false"
                    },
                    "deletingDate": {
                        "type": "date",
                        "description": "If disable=false - when did it happen if not - date=null?",
                        "example": "03-06-2023"
                    }
                }
            },
            "orderer": {
                "type": "object",
                "properties": {
                    "serialNumber": {
                        "type": "int",
                        "description": "Filles it self",
                        "example": "56"
                    },
                    "name": {
                        "type": "string",
                        "description": "The orderer's name (up to 20 chars)",
                        "example": "משה"
                    },
                    "phone": {
                        "type": "string",
                        "description": "The orderer's phone (9 or 10 chars)",
                        "example": "0527167844"
                    },
                    "addedDate": {
                        "type": "date",
                        "description": "The date of the orderer's first order",
                        "example": "new Date()"
                    },
                    "disable": {
                        "type": "boolean",
                        "description": "Is the orderer able to make an order?",
                        "example": "false"
                    },
                    "deletingDate": {
                        "type": "date",
                        "description": "If disable=false - when did it happen if not - date=null?",
                        "example": "new Date()"
                    }
                }
            },
            "createnewlead": {
                "type": "object",
                "properties": {
                    "serialNumber": {
                        "type": "int",
                        "description": "Filles it self",
                        "example": "56"
                    },
                    "name": {
                        "type": "string",
                        "description": "The status name (up to 20 chars)",
                        "example": "משה"
                    },
                    "addedDate": {
                        "type": "date",
                        "description": "The date when the status was added",
                        "example": "new Date()"
                    },
                    "disable": {
                        "type": "boolean",
                        "description": "Is the status able?",
                        "example": "false"
                    },
                    "deletingDate": {
                        "type": "date",
                        "description": "If disable=false - when did it happen? if not - date=null",
                        "example": "new Date()"
                    }
                }
            }
           

           
     
       
       
    }
